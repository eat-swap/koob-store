<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/nav_graph_home"
	app:startDestination="@id/page_browse">

	<fragment
		android:id="@+id/page_browse"
		android:name="org.eatswap.koobstore.modules.home.BrowseFragment"
		android:label="fragment_browse"
		tools:layout="@layout/fragment_browse" >
		<action
			android:id="@+id/action_page_browse_to_detailsFragment"
			app:destination="@id/detailsFragment"
			app:enterAnim="@anim/nav_default_enter_anim"
			app:exitAnim="@anim/nav_default_exit_anim"
			app:popUpTo="@id/page_browse"
			app:popUpToInclusive="true"
			app:popEnterAnim="@anim/nav_default_pop_enter_anim"
			app:popExitAnim="@anim/nav_default_pop_exit_anim" />
	</fragment>
	<fragment
		android:id="@+id/detailsFragment"
		android:name="org.eatswap.koobstore.modules.home.ui.home.DetailsFragment"
		android:label="fragment_details"
		tools:layout="@layout/fragment_details" >
		<argument
			android:name="bookId"
			app:argType="string" />
	</fragment>
</navigation>